      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fixedveil.Foco"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9ca2971c-f0e9-4246-9ebc-3fbb541a53b6"
      }
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
